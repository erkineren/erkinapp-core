{
  "name": "erkineren/erkinapp-core",
  "type": "library",
  "description": "A Multipurpose PHP Framework Core",
  "keywords": [
    "erkinapp",
    "framework"
  ],
  "license": "GPL-3.0+",
  "authors": [
    {
      "name": "Erkin Eren",
      "email": "eren@erkin.net",
      "homepage": "https://erkin.net"
    }
  ],
  "config": {
    "autoloader-suffix": "ErkinApp",
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2"
    },
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "cocur/slugify": "^3.1",
    "danielstjules/stringy": "^3.1",
    "doctrine/annotations": "^1.7",
    "envms/fluentpdo": "^2.1.0",
    "filp/whoops": "^2.3",
    "guzzlehttp/guzzle": "^6.3",
    "matthiasmullie/minify": "^1.3",
    "monolog/monolog": "^1.24",
    "php-curl-class/php-curl-class": "^8.4",
    "phpmailer/phpmailer": "^6.0",
    "pimple/pimple": "^3.2",
    "symfony/config": "^4.2",
    "symfony/dependency-injection": "^4.2",
    "symfony/event-dispatcher": "^4.2",
    "symfony/expression-language": "^4.2",
    "symfony/finder": "^4.2",
    "symfony/http-foundation": "^4.2",
    "symfony/http-kernel": "^4.2",
    "symfony/routing": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "ErkinApp\\": "system/core"
    },
    "classmap": [
      "system/libraries/"
    ],
    "files": [
      "system/include.php"
    ]
  }
}
